{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node",
            "command": "npm i --legacy-peer-deps"
        }
    ],
    "start": {
        "src": "index.js",
        "use": "@vercel/node",
        "command": "node index.js"
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}